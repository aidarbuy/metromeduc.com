{
  "firebase": "metromeduc",
  "public": "dist",
  "ignore": [
    ".tmp",
    "**/bash/**",
    "**/bower_components/**",
    "**/e2e/**",
    "**/gulp/**",
    "**/node_modules/**",
    "**/server",
    "**/src/**",
    "firebase.json",
    "**/.*"
  ],
  "rewrites": [{
    "source": "**",
  	"destination": "/index.html"
  }]
}